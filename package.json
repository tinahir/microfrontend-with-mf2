{
  "name": "microfrontend-with-mf2",
  "description": "Demo of fetching a React component in a Vue app via Module Federation, showcasing two-way communication and lifecycle management.",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "preinstall": "pnpm install --ignore-scripts",
    "start": "pnpm run --filter microfrontend-with-mf2_** --parallel dev",
    "build": "pnpm run --filter microfrontend-with-mf2_* build",
    "serve": "pnpm run --filter microfrontend-with-mf2_* --parallel serve",
    "clean": "pnpm run --filter microfrontend-with-mf2_* --parallel clean"
  },
  "devDependencies": {
    "@babel/preset-react": "7.24.7",
    "wait-on": "7.2.0"
  }
}
